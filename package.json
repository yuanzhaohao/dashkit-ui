{
  "name": "dashkit-ui",
  "description": "UI Components built on React.",
  "version": "1.0.0",
  "main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "author": "yuanzhaohao",
  "keywords": [
    "ui",
    "react",
    "component"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "node scripts/dev.js",
    "site": "node scripts/docs.js && gh-pages -d gh-pages",
    "pub": "node scripts/publish.js && npm run pub:tsEs && npm run pub:tsLib",
    "pub:tsEs": "tsc -p tsconfig.publish.json",
    "pub:tsLib": "tsc -p tsconfig.publish.json --declarationDir publish/lib",
    "format": "prettier --parser typescript './{src,site}/**/*.{ts,tsx}' --write",
    "format:scripts": "prettier './scripts/*.js' --write",
    "lint": "tslint tsconfig.json './{src,site}/**/*.{ts,tsx}'",
    "lint:fix": "tslint --project tsconfig.json './{src,site}/**/*.{ts,tsx}' --fix",
    "test": "jest --config jest.config.js --no-cache"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "tslint --project tsconfig.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuanzhaohao/dashkit-ui.git"
  },
  "homepage": "http://yuanzhaohao.github.io/dashkit-ui/#/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.5",
    "lodash-es": "^4.17.11",
    "react-transition-group": "^2.4.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.4",
    "@types/jest": "^24.0.13",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-transition-group": "^2.0.14",
    "@types/warning": "^3.0.0",
    "autoprefixer": "^8.6.3",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.1",
    "clean-css": "^4.1.11",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "csso": "^3.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^6.0.1",
    "gh-pages": "^2.1.1",
    "glob": "^7.1.2",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "js-yaml": "^3.13.1",
    "lint-staged": "^8.1.6",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.2",
    "markdown-website-loader": "0.0.7",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.1.0",
    "portfinder": "^1.0.13",
    "postcss": "^7.0.17",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.2.2",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.4.2",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "uglify-js": "^3.4.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.3"
  }
}
